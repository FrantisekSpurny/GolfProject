        android:layout_alignLeft="@+id/textView11"
        android:layout_alignStart="@+id/textView11" />

    <ImageView
        android:layout_width="100dp"
        android:layout_height="20dp"
        android:id="@+id/imageView6"
        android:layout_marginTop="7dp"
