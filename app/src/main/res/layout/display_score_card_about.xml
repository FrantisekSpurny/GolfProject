        android:layout_alignStart="@+id/textView14" />

    <ImageView
        android:layout_width="100dp"
        android:layout_height="20dp"
        android:id="@+id/imageView9"
        android:layout_marginTop="7dp"
        android:background="@drawable/triple_bogey"
        android:layout_below="@+id/imageView8"
        android:layout_alignLeft="@+id/imageView8"
        android:layout_alignStart="@+id/imageView8" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="@string/DisplayScoreCardAbout_string_tripleBogey"
        android:id="@+id/textView16"
        android:layout_alignBottom="@+id/imageView9"
        android:layout_alignLeft="@+id/textView15"
        android:layout_alignStart="@+id/textView15" />

    <ImageView
        android:layout_width="100dp"
        android:layout_height="20dp"
        android:id="@+id/imageView10"
        android:layout_marginTop="7dp"
        android:background="@drawable/albatros"
        android:layout_below="@+id/imageView9"
        android:layout_alignLeft="@+id/imageView9"
        android:layout_alignStart="@+id/imageView9" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="@string/DisplayScoreCardAbout_string_other"
        android:id="@+id/textView17"
        android:layout_alignBottom="@+id/imageView10"
        android:layout_alignLeft="@+id/textView16"
        android:layout_alignStart="@+id/textView16" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/ok"
        android:id="@+id/DisplayScoreCardAbout_button_ok"
        android:layout_below="@+id/imageView10"
        android:layout_alignLeft="@+id/imageView10"
        android:layout_alignStart="@+id/imageView10"
        android:layout_marginTop="15dp"
        android:layout_marginRight="20dp"
        android:layout_alignParentRight="true"/>
