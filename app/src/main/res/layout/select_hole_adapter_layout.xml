<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginBottom="10dp"
    android:layout_marginTop="10dp"
    android:orientation="vertical">

    <TextView
        android:id="@+id/SelectHoleAdapter_textView_number"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/SelectHoleAdapter_textView_nameTitle"
        android:layout_marginLeft="10dp"
        android:text=""
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/SelectHoleAdapter_textView_parTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="10dp"
        android:layout_toEndOf="@+id/SelectHoleAdapter_textView_number"
        android:layout_toRightOf="@+id/SelectHoleAdapter_textView_number"
        android:text="@string/SelectHoleAdapter_string_par"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/SelectHoleAdapter_textView_nameTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/SelectHoleAdapter_textView_parTitle"
        android:layout_alignStart="@+id/SelectHoleAdapter_textView_parTitle"
        android:layout_below="@+id/SelectHoleAdapter_textView_parTitle"
        android:text="@string/SelectHoleAdapter_string_name"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/SelectHoleAdapter_textView_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/SelectHoleAdapter_textView_nameTitle"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="16dp"
        android:layout_toEndOf="@+id/SelectHoleAdapter_textView_nameTitle"
        android:layout_toRightOf="@+id/SelectHoleAdapter_textView_nameTitle"
        android:text=""
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/SelectHoleAdapter_textView_par"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/SelectHoleAdapter_textView_name"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/SelectHoleAdapter_textView_name"
        android:text=""
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/SelectHoleAdapter_textView_length"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:text=""
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/SelectHoleAdapter_textView_lengthTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_toLeftOf="@+id/SelectHoleAdapter_textView_length"
        android:layout_toStartOf="@+id/SelectHoleAdapter_textView_length"
        android:text="@string/SelectHoleAdapter_string_lenght"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/SelectHoleAdapter_textView_hcpTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/SelectHoleAdapter_textView_nameTitle"
        android:layout_alignStart="@+id/SelectHoleAdapter_textView_nameTitle"
        android:layout_below="@+id/SelectHoleAdapter_textView_nameTitle"
        android:text="@string/SelectHoleAdapter_string_hcp"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/SelectHoleAdapter_textView_hcp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/SelectHoleAdapter_textView_name"
        android:layout_alignStart="@+id/SelectHoleAdapter_textView_name"
        android:layout_alignTop="@+id/SelectHoleAdapter_textView_hcpTitle"
        android:text=""
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <View
        android:id="@+id/imageView"
        android:layout_width="fill_parent"
        android:layout_height="1dip"
        android:layout_alignEnd="@+id/SelectHoleAdapter_textView_length"
        android:layout_alignLeft="@+id/SelectHoleAdapter_textView_hcpTitle"
        android:layout_alignRight="@+id/SelectHoleAdapter_textView_length"
        android:layout_alignStart="@+id/SelectHoleAdapter_textView_hcpTitle"
        android:layout_below="@+id/SelectHoleAdapter_textView_hcpTitle"
        android:layout_marginBottom="5dp"
        android:layout_marginTop="5dp"
        android:background="#14000000" />

    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignLeft="@+id/imageView"
        android:layout_alignStart="@+id/imageView"
        android:layout_below="@+id/imageView">

        <TableRow>


            <TextView
                android:id="@+id/SelectHoleAdapter_textView_player1Title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textAppearance="?android:attr/textAppearanceSmall" />


            <TextView
                android:id="@+id/SelectHoleAdapter_textView_player1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textAppearance="?android:attr/textAppearanceSmall" />
        </TableRow>

        <TableRow>

            <TextView
                android:id="@+id/SelectHoleAdapter_textView_player2Title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:visibility="gone" />

            <TextView
                android:id="@+id/SelectHoleAdapter_textView_player2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:visibility="gone" />
        </TableRow>

        <TableRow>

            <TextView
                android:id="@+id/SelectHoleAdapter_textView_player3Title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:visibility="gone" />

            <TextView
                android:id="@+id/SelectHoleAdapter_textView_player3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:visibility="gone" />
        </TableRow>

        <TableRow>

            <TextView
                android:id="@+id/SelectHoleAdapter_textView_player4Title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:visibility="gone" />

            <TextView
                android:id="@+id/SelectHoleAdapter_textView_player4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:visibility="gone" />
        </TableRow>

    </TableLayout>

</RelativeLayout>